# VulnAPI Configuration
# Copy this file to .env and adjust values

# Mode: challenge (default) or documentation
VULNAPI_MODE=challenge

# Database URL
VULNAPI_DB_URL=sqlite+aiosqlite:///./vulnapi.db

# Secret key for JWT (intentionally weak for educational purposes)
VULNAPI_SECRET_KEY=intentionally-weak-secret-key-do-not-use-in-production

# Debug mode
VULNAPI_DEBUG=true

# API version
VULNAPI_API_VERSION=v1
